#include <stdio.h>

int main() {
    int n;
    long long product = 1; // Use long long for large numbers

    printf("Enter a positive integer n: ");
    scanf("%d", &n);

    for (int i = 2; i <= n; i += 2) {
        product *= i;
    }

    if (n < 2)
        printf("No even numbers in the range 1 to %d.", n);
    else
        printf("Product of even numbers from 1 to %d = %lld", n, product);

    return 0;
}

