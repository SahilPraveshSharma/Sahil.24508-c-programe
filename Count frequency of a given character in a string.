#include <stdio.h>
#include <ctype.h>  

int main() {
    char str[1000], ch;
    int count = 0;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    printf("Enter the character to find frequency: ");
    scanf("%c", &ch);

    char target = tolower(ch);

    for (int i = 0; str[i] != '\0'; i++) {
        if (tolower(str[i]) == target) {
            count++;
        }
    }

    printf("Frequency of '%c' = %d\n", ch, count);

    return 0;
}
