#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    int length;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    str[strcspn(str, "\n")] = '\0';

    length = strlen(str);

    printf("All substrings of the string:\n");

    for (int i = 0; i < length; i++) {        
        for (int j = i; j < length; j++) {   
            for (int k = i; k <= j; k++) {      
                printf("%c", str[k]);
            }
            printf("\n");
        }
    }

    return 0;
}
