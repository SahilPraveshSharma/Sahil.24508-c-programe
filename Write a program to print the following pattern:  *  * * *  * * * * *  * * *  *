#include <stdio.h>

int main() {
    int groups[] = {1, 3, 5, 3, 1}; 
    int totalGroups = sizeof(groups)/sizeof(groups[0]);

    for (int i = 0; i < totalGroups; i++) {
        for (int j = 0; j < groups[i]; j++) {
            printf("*\n");
        }
        printf("\n"); // Blank line after each group
    }

    return 0;
}
